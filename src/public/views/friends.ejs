<%- include('include/head.ejs')%>
    <title>Friends - Kokoa Talk</title>
</head>
<body>
    <%- include('include/status_bar.ejs')%>
    <header class="screen-header">
        <h1 class="screen-header__title">Friends</h1>
        <div class="screen-header__icons">
            <span> <i class="fas fa-search fa-lg"></i></span>
            <span><i class="fas fa-music fa-lg"></i></span>
            <span><i class="fas fa-cog fa-lg"></i></span>
        </div>
    </header>
    <a id="friends-display-link">
        <i class="fas fa-info-circle"></i>Friends' Names Display
        <i class="fas fa-chevron-right fa-xs"></i>
    </a>
    <main class="main-screen">
        <div class="user-component">
            <div class="user-component__column">
                <img src="<%=profileImg%>" 
                     class="user-component__profile user-component__profile--xl"/>
                <div class="user-component__text">
                    <h4 class="user-component__title"><%=userNick%></h4>
                    <!-- <h6 class="user-component__subtitle">this text whatever</h6> -->
                </div>
            </div>
            <div class="user-component__column"></div>
        </div>
        <div class="main__list">
            <div class="main__list-header">
                <span>Channel</span>
                <i class="fas fa-chevron-up fa-xs"></i>
            </div>
        </div>
        <div class="main__list-component">
            <div class="main__list-component__column">
                <img src="data:image/png;base64," 
                     class="main__list__profile"/>
                <div class="main__list__text">
                    <h4 class="main__list__title">Channel</h4>
                </div>
            </div>
            <div class="main__list-component__column">
                <span>2</span>
                <i class="fas fa-chevron-right fa-xs"></i>
            </div>
        </div>
        <div class="main__list">
            <div class="main__list-header-freinds">
                <span>Friends</span>
                <span>2</span>
                <i class="fas fa-chevron-up fa-xs"></i>
            </div>
        </div>
       <div class="main__list-component list-reapeat">
            <div class="main__list-component__column">
                <img src="https://img.insight.co.kr/static/2020/12/13/700/img_20201213152823_hh576838.webp" 
                     class="main__list__profile"/>
                <div class="main__list__text">
                    <h4 class="main__list__title">Jimin</h4>
                    <h6 class="main__list__profile__subtitle">this text whatever</h6>
                </div>
            </div>
        </div> 
        <div class="main__list-component list-reapeat">
            <div class="main__list-component__column">
                <img src="http://file3.instiz.net/data/cached_img/upload/2021/01/27/18/2ce2f41f7e9f09c0acc77faac7452dbf.jpg" 
                     class="main__list__profile"/>
                <div class="main__list__text">
                    <h4 class="main__list__title">루피</h4>
                    <h6 class="main__list__profile__subtitle">인생은 쓰고 술은 달다</h6>
                </div>
            </div>
        </div> 
    </main>
    <%- include('include/tabbar.ejs')%>
    <!-- <div id="splash-screen">
        <i class="fas fa-comment"></i>
    </div> -->
    
<%- include('include/tail.ejs')%>